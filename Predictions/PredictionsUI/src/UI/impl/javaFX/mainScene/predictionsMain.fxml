<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="650.0" minWidth="800.0" prefHeight="650.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.impl.javaFX.mainScene.PredictionsMainController">
   <center>
      <TabPane fx:id="mainTabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab onCloseRequest="#executionTabClicked" onClosed="#resultsTabClicked" onSelectionChanged="#detailsTabClicked" text="Details">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="1200.0">
                     <children>
                        <fx:include fx:id="detailsComponent" source="../tabBody/details/PredictionsDetailsIGrid.fxml" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab onSelectionChanged="#executionTabClicked" text="Execution">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <fx:include fx:id="newExecutionComponent" source="../tabBody/newExecution/newExecutionMain/newExecution.fxml" />
                     </children></AnchorPane>
            </content>
          </Tab>
            <Tab onSelectionChanged="#resultsTabClicked" text="Results">
               <content>
                  <fx:include fx:id="resultsComponent" source="../tabBody/results/ResultsGrid.fxml" />
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </center>
   <bottom>
      <AnchorPane maxHeight="30.0" minHeight="30.0" prefHeight="30.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </bottom>
   <top>
      <fx:include fx:id="topComponent" source="../top/PredictionsTop.fxml" />
   </top>
</BorderPane>
